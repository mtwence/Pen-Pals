      username: \"\",\n      email: \"\",\n      password: \"\",\n    });\n  };\n\n  return (\n    <div class=\"col-4 container h-100\">\n      <h2>Signup</h2>\n      <Form\n        class=\"mx-1 mx-md-4\"\n        noValidate\n        validated={validated}\n        onSubmit={handleFormSubmit}\n      >\n        {/* show alert if server response is bad */}\n        <Alert\n          dismissible\n          onClose={() => setShowAlert(false)}\n          show={showAlert}\n          variant=\"danger\"\n        >\n          Something went wrong with your signup!\n        </Alert>\n        <div class=\"d-flex flex-row align-items-center mb-4\">\n          <i class=\"fa fa-user fa-lg me-3 fa-fw\"></i>\n          <Form.Group>\n            <div class=\"form-outline flex-fill mb-0\">\n              <Form.Control\n                placeholder=\"What will be your username?\"\n                name=\"username\"\n                type=\"username\"\n                id=\"username\"\n                class=\"form-control\"\n                onChange={handleInputChange}\n                value={userFormData.username}\n                required\n              />\n              <Form.Control.Feedback type=\"invalid\">\n                Please enter a name.\n              </Form.Control.Feedback>\n              <Form.Label htmlFor=\"username\">Username:</Form.Label>\n            </div>\n          </Form.Group>\n        </div>\n        <div class=\"d-flex flex-row align-items-center mb-4\">\n          <i class=\"fa fa-envelope fa-lg me-3 fa-fw\"></i>\n          <div class=\"form-outline flex-fill mb-0\">\n            <Form.Control\n              placeholder=\"What is your email?\"\n              name=\"email\"\n              type=\"email\"\n              id=\"email\"\n              class=\"form-control\"\n              onChange={handleInputChange}\n              value={userFormData.email}\n              required\n            />\n            <Form.Control.Feedback type=\"invalid\">\n              Please enter an email.\n            </Form.Control.Feedback>\n            <Form.Label htmlFor=\"email\">Email:</Form.Label>\n          </div>\n        </div>\n        <div class=\"d-flex flex-row align-items-center mb-4\">\n          <i class=\"fa fa-lock fa-lg me-3 fa-fw\"></i>\n          <div class=\"form-outline flex-fill mb-0\">\n            <Form.Control\n              placeholder=\"What password will you set?\"\n              name=\"password\"\n              type=\"password\"\n              id=\"password\"\n              class=\"form-control\"\n              onChange={handleInputChange}\n              value={userFormData.password}\n              required\n            />\n            <Form.Control.Feedback type=\"invalid\">\n              Please enter a password.\n            </Form.Control.Feedback>\n            <Form.Label htmlFor=\"password\">Password:</Form.Label>\n          </div>\n        </div>\n        <div class=\"d-flex justify-content-center mx-4 mb-3 mb-lg-4\">\n          <Button\n            disabled={\n              !(\n                userFormData.username &&\n                userFormData.email &&\n                userFormData.password\n              )\n            }\n            type=\"submit\"\n            variant=\"success\"\n            class=\"btn btn-primary btn-lg\"\n          >\n            Register\n          </Button>\n        </div>\n      </Form>\n    </div>\n  );\n}\n\nexport default Signup;\n","C:\\Users\\mtwen\\Desktop\\Pen-Pals\\client\\src\\components\\LoginSignup\\Login.js",[],"C:\\Users\\mtwen\\Desktop\\Pen-Pals\\client\\src\\utils\\auth.js",[],"C:\\Users\\mtwen\\Desktop\\Pen-Pals\\client\\src\\utils\\mutations.js",[],{"ruleId":"56","severity":1,"message":"57","line":1,"column":17,"nodeType":"58","messageId":"59","endLine":1,"endColumn":25},{"ruleId":"56","severity":1,"message":"60","line":1,"column":27,"nodeType":"58","messageId":"59","endLine":1,"endColumn":36},{"ruleId":"56","severity":1,"message":"57","line":1,"column":17,"no